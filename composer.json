{
    "name": "sysf/sysf",
    "description": "Multi-tenant system everything.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "repositories": {
        "ckeditor/templates": {
            "type": "package",
            "package": {
                "name": "ckeditor/templates",
                "type": "drupal-library",
                "version": "4.11.1",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/templates/releases/templates_4.11.1.zip",
                    "reference": "master"
                }
            }
        },
        "ckeditor/colorbutton": {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "type": "drupal-library",
                "version": "4.11.4",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.11.4.zip",
                    "reference": "master"
                }
            }
        },
        "ckeditor/panelbutton": {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "type": "drupal-library",
                "version": "4.11.4",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.11.4.zip",
                    "reference": "master"
                }
            }
        },
        "ckeditor/font": {
            "type": "package",
            "package": {
                "name": "ckeditor/font",
                "type": "drupal-library",
                "version": "4.11.4",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/font/releases/font_4.11.4.zip",
                    "reference": "master"
                }
            }
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "acquia/blt": "^12.0",
        "bower-asset/d3": "v3.5.17",
        "ckeditor/colorbutton": "^4.11",
        "ckeditor/font": "^4.11",
        "ckeditor/panelbutton": "^4.11",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advagg": "^4.0",
        "drupal/ckeditor_font": "^1.0",
        "drupal/ckeditor_templates": "^1.1",
        "drupal/ckeditor_templates_ui": "^1.0",
        "drupal/coder": "^8.3",
        "drupal/color_field": "^2.0",
        "drupal/colorbutton": "^1.1",
        "drupal/commerce": "^2.11",
        "drupal/console": "^1.8",
        "drupal/crop": "*",
        "drupal/csv_serialization": "^1.4.0",
        "drupal/ctools": "^3.1",
        "drupal/devel": "^4.0",
        "drupal/devel_entity_updates": "*",
        "drupal/diff": "^1.0@RC",
        "drupal/domain": "^1.0@beta",
        "drupal/domain_entity": "^1.0@beta",
        "drupal/domain_group": "^2.0",
        "drupal/dropzonejs": "^2.0@alpha",
        "drupal/dynamic_entity_reference": "dev-2.x",
        "drupal/efs": "*",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/email_registration": "^1.1",
        "drupal/embed": "^1.0",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/entity_share": "^3.0@beta",
        "drupal/entity_usage": "^2.0@alpha",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/erd": "^1.0@beta",
        "drupal/exif_orientation": "^1.0",
        "drupal/field_group": "dev-3.x",
        "drupal/field_tools": "^1.0@alpha",
        "drupal/field_validation": "^1.0@alpha",
        "drupal/fontawesome": "^2.13",
        "drupal/fontyourface": "^3.2",
        "drupal/geocoder": "^2.0@beta",
        "drupal/geofield": "^1.0@beta",
        "drupal/google_analytics": "^2.3",
        "drupal/group": "^1.3",
        "drupal/image_effects": "^3.0",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "dev-1.x",
        "drupal/jsonapi_explorer": "^1.1",
        "drupal/jsonapi_extras": "^3.17",
        "drupal/jsonapi_include": "^1.4",
        "drupal/jsonapi_resources": "^1.0@beta",
        "drupal/jsonapi_search_api": "^1.0@RC",
        "drupal/jsonapi_views": "^1.0@beta",
        "drupal/key_value_field": "^1.0",
        "drupal/keycloak": "dev-1.x",
        "drupal/layout_builder_iframe_modal": "^1.1",
        "drupal/length_indicator": "^1.1",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "*",
        "drupal/mailsystem": "^4.3",
        "drupal/maxlength": "^1.0@beta",
        "drupal/media_directories": "^2.0",
        "drupal/menu_item_extras": "2.15.0",
        "drupal/metatag": "^1.8",
        "drupal/migrate_plus": "^5.1",
        "drupal/page_manager": "dev-4.x",
        "drupal/panelbutton": "^1.2",
        "drupal/pathauto": "^1.3",
        "drupal/plugin": "^2.5",
        "drupal/purge": "^3.0@beta",
        "drupal/qrfield": "dev-1.x",
        "drupal/recommended-project": "^9.2",
        "drupal/redirect": "^1.3",
        "drupal/redis": "dev-1.x",
        "drupal/rules": "dev-3.x",
        "drupal/scheduled_updates": "dev-1.x",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/select2_all": "^1.0@alpha",
        "drupal/sendgrid_api": "^1.0@alpha",
        "drupal/simple_oauth": "^5.0",
        "drupal/slick_entityreference": "^2.0",
        "drupal/slick_views": "^2.0",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/ui_patterns": "^1.0@RC",
        "drupal/ui_patterns_layout_builder": "1.x-dev",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/url_embed": "^1.0",
        "drupal/views_ajax_history": "^1.0",
        "drupal/views_bulk_edit": "^2.2",
        "drupal/views_bulk_operations": "^4.0-rc1",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/xhprof": "1.x-dev",
        "drupal/yoast_seo": "^1.7",
        "league/csv": "^8.2",
        "luizbills/css-generator": "^3.1",
        "npm-asset/blazy": "^1.8",
        "npm-asset/cropper": "^4.1",
        "npm-asset/diff": "^3.5",
        "npm-asset/dropzone": "^5.7",
        "npm-asset/jstree": "^3.3",
        "npm-asset/select2": "^4.0",
        "npm-asset/slick-carousel": "^1.8",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Sysf\\": "blt/src/"
        }
    },
    "scripts": {
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ],
        "blt-alias": "blt blt:init:shell-alias -y --ansi",
        "install-phantomjs": [
            "rm -rf vendor/bin/phantomjs",
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ComposerHandler::symlinkDrushVersionSupport"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ComposerHandler::symlinkDrushVersionSupport"
        ],
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ]
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": false,
        "installer-types": [
            "bower-asset",
            "npm-asset",
            "drupal-library"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/modules/custom/pww/{$name}": [
                "type:pww-component"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "allowed-packages": [
                "drupal/core"
            ],
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/.htaccess": false
            }
        },
        "patches": {
            "drupal/core": {
                "entity-reference-views-selection-workaround-and-ids": "./patches/entity-reference-views-selection-workaround.patch",
                "layout-builder-region-weight-workaround": "./patches/layout-builder-region-weight-workaround.patch",
                "3080606-reorder-layout-sections-43": "https://www.drupal.org/files/issues/2020-06-18/3080606-reorder-layout-sections-43.patch",
                "entity-create-duplicate-provide-source-entity-property": "https://git.drupalcode.org/project/drupal/-/merge_requests/204.diff"
            },
            "drupal/plugin": {
                "use-subform-state-support-config-schema-subform": "https://www.drupal.org/files/issues/2020-07-22/2647312-32.patch"
            },
            "drupal/entity_browser": {
                "entity_browser-allowed_bundles_entity_form_widget-2765295-22": "https://www.drupal.org/files/issues/2021-03-25/entity_browser-allowed_bundles_entity_form_widget-2765295-22.patch",
                "base-fields-target-bundle-null": "https://www.drupal.org/files/issues/2019-08-15/entity_browser-3075119.patch"
            },
            "drupal/scheduled_updates": {
                "drupal-9-readiness": "https://www.drupal.org/files/issues/2021-06-18/3172330-31.patch",
                "content-moderation-support": "https://www.drupal.org/files/issues/2020-09-29/scheduled_updates_with_content_moderation-2821916-32.patch"
            },
            "drupal/rules": {
                "rules-event-forms-3010055-16": "https://www.drupal.org/files/issues/2020-08-03/rules-event-forms-3010055-16.patch",
                "https://www.drupal.org/files/issues/2021-04-23/rules-2664280-82.select-lists.patch": "./patches/rules-select-will-not-combine-with-multi-event-patch.patch"
            },
            "drupal/redis": {
                "redis-cluster-support": "https://www.drupal.org/files/issues/2021-06-07/2900947-49.patch"
            },
            "drupal/elasticsearch_connector": {
                "elasticsearch_connector-hide-system-indices-3229401": "https://www.drupal.org/files/issues/2021-08-25/elasticsearch_connector-hide-system-indices-3229401.patch"
            },
            "drupal/oauth2_server": {
                "remove-entity-reference-dependency-d9": "https://git.drupalcode.org/project/oauth2_server/-/merge_requests/2.diff"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    }
}
